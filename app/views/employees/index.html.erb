<h1>Employees</h1>

<div id="employees">
  <% @employees.each do |employee| %>
    <%= render employee %>
    <p>
      <%= link_to "Show this employee", employee %> || 
      <%= link_to "Edit this employee", edit_employee_path(employee) %>
    </p>
  <% end %>
</div>

